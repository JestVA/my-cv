
-
  var header = '#recent clients'
  var clientList = [
    "/asset/image/client/bitbee-logo.png",
    "/asset/image/client/clickminded-logo.png",
    "/asset/image/client/fifa18-logo.png",
    "/asset/image/client/logo-upwork.jpg",
  ];

#client
.client
  .client__header=header
  .client__list
    each client in clientList
      .client__image
        img(src=client)